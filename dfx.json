{
  "version": 1,
  "canisters": {
    "emoji_drop_backend": {
      "type": "motoko",
      "main": "src/emoji_drop_backend/main.mo"
    },
    "emoji_drop_frontend": {
      "dependencies": [
        "emoji_drop_backend"
      ],
      "type": "assets",
      "frontend": {
        "entrypoint": "src/emoji_drop_frontend/dist/index.html"
      },
      "source": [
        "src/emoji_drop_frontend/dist/"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env"
}